/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "target": "ES2022"
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "exclude": [
    "src/**/*.unused.(ts|js)",
    "src/**/*.i.spec.(ts|js)",
    "src/**/*.u.spec.(ts|js)",
    "src/**/*.spec.(ts|js)"
  ]
}
